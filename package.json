{
  "name": "token-search",
  "author": "github@omaraziz.dev",
  "homepage": "https://token-search-production.up.railway.app",
  "repository": "github:o-az/token-search",
  "scripts": {
    "prestart": "[ -f tokens.sqlite ] || (echo 'you need to run `bun setup` first' && exit 2)",
    "start": "bun run --hot src/index.ts",
    "setup": "bash ./scripts/setup.sh",
    "lint": "eslint --fix . --ext .js,.cjs,.mjs,.ts --cache",
    "format": "prettier --config .prettierrc.cjs . --ignore-path .eslintignore --write --ignore-unknown",
    "clean": "sh ./scripts/clean.sh || true"
  },
  "dependencies": {
    "hono": "^3.0.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.21.1",
    "@types/node": "^18.13.0",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "bun-types": "^0.5.6",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5"
  }
}
